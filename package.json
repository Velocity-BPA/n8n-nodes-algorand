{
  "name": "n8n-nodes-algorand",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Algorand blockchain providing 8 resources and 86+ operations for accounts, transactions, ASAs, NFTs, smart contracts, and more.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-algorand.git"
  },
  "homepage": "https://velobpa.com",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "algorand",
    "blockchain",
    "algo",
    "asa",
    "nft",
    "defi",
    "smart-contracts",
    "web3",
    "atomic-transfers"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AlgorandNetwork.credentials.js",
      "dist/credentials/AlgorandIndexer.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Algorand/Algorand.node.js",
      "dist/nodes/Algorand/AlgorandTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "algosdk": "^2.7.0",
    "axios": "^1.6.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
