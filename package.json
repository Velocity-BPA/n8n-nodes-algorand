{
  "name": "n8n-nodes-algorand",
  "version": "1.0.0",
  "description": "n8n community node for Algorand",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://n8n.io",
  "author": {
    "name": "Velocity BPA",
    "email": "support@velocitybpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/velocitybpa/n8n-nodes-algorand.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "dist/nodes/Algorand/Algorand.node.js",
  "files": [
    "dist/**/*"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AlgorandApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Algorand/Algorand.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "algorand",
    "blockchain",
    "crypto",
    "smart-contracts"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "format": "prettier nodes credentials --write",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.50.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.5.0"
  }
}